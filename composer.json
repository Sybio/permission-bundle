{
    "name": "sybio/permission-bundle",
    "description": "A Symfony bundle to manage permissions as Value Objects.",
    "homepage": "https://github.com/Sybio/permission-bundle",
    "type": "library",
    "license": "MIT",
    "keywords": ["permission", "voter", "security", "symfony", "role"],
    "authors": [
        {
            "name": "Sybio (ClÃ©ment Guillemain)",
            "email": "permission-bundle@clementguillemain.com"
        }
    ],
    "require": {
        "php": ">=8.3",
        "symfony/config": "^7.1",
        "symfony/dependency-injection": "^7.1",
        "symfony/http-kernel": "^7.1",
        "symfony/security-core": "^7.1",
        "symfony/twig-bundle": "^7.1",
        "symfony/validator": "^7.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.92",
        "phpstan/phpstan": "^2.1",
        "symfony/test-pack": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Sybio\\PermissionBundle\\": "src/",
            "Sybio\\PermissionBundle\\Tests\\": "tests/"
        }
    },
    "autoload-dev": {},
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "prefer-stable": true,
    "support": {
        "issues": "https://github.com/Sybio/permission-bundle/issues",
        "source": "https://github.com/Sybio/permission-bundle",
        "docs": "https://github.com/Sybio/permission-bundle/blob/main/README.md"
    }
}
